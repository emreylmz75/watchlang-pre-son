    "plugins": [
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        }
        
      }]
    ],
    
    "slug": "watchlang-pre",
   
    
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
